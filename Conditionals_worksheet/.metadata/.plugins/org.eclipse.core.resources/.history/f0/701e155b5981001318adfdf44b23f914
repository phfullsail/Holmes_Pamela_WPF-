//if weight if greater than 250lbs entrant qualifies for the heavyweight division

var eWeight = 200;
var rWeight = 250;

if(eWeight >= rWeight) {
	console.log("The competitor qualifies for the heavyweight division.");
}else {
	console.log("he competitor needs to gain some weight!");
}



